{
  "name": "finding-falcone",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://sambhav2612.github.io/finding-falcone",
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.7.3",
    "axios": "^0.21.0",
    "query-string": "^6.13.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development react-scripts start",
    "clean": "node node_modules/gh-pages/bin/gh-pages-clean.js",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "test": "cross-env NODE_ENV=development react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
